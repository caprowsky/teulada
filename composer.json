{
    "name": "my/project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "php": "^5.5.9|>=7.0.8",
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5.4",
        "grasmash/drupal-security-warning": "^1.0.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "wikimedia/composer-merge-plugin": "^1.4.1",
        "drupal/search_api_solr": "^1.0.0",
        "drupal/paragraphs": "^1.1.0",
        "drupal/mailchimp": "^1.3.0",
        "drupal/smtp": "^1.0.0-beta3",
        "drupal/facets": "^1.0.0-alpha11",
        "drupal/field_validation": "^1.0.0-alpha5",
        "drupal/rabbit_hole": "^1.0.0-beta3",
        "drupal/masonry": "^1.0.0-rc2",
        "drupal/youtube": "^1.0.0-beta3",
        "drupal/field_group": "^1.0.0-rc6",
        "drupal/back_to_top": "^1.0.0-beta2",
        "drupal/devel": "^1.0.0-rc2",
        "drupal/addtoany": "^1.8.0",
        "drupal/theme_change": "^1.5.0",
        "drupal/views_data_export": "^1.0.0-alpha4",
        "drupal/pathauto": "^1.0.0",
        "drupal/role_delegation": "^1.0.0-alpha1",
        "drupal/entity": "^1.0.0-alpha4",
        "drupal/viewsreference": "^1.0.0",
        "drupal/simple_sitemap": "^2.9.0",
        "drupal/search_api_glossary": "^3.2.0",
        "drupal/metatag": "^1.2.0",
        "drupal/entityqueue": "^1.0.0-alpha6",
        "drupal/search_api_pantheon": "^1.0.0-alpha3",
        "drupal/blazy": "^1.0.0-rc2",
        "drupal/panelizer": "^4.0.0",
        "drupal/token": "^1.0.0",
        "drupal/restui": "^1.15.0",
        "drupal/views_accordion": "^1.0.0",
        "drupal/admin_toolbar": "^1.19.0",
        "drupal/block_visibility_groups": "^1.1.0",
        "drupal/adv_varnish": "^2.0.0",
        "drupal/twig_tweak": "^1.8.0",
        "drupal/lang_dropdown": "^2.0.0-beta1",
        "drupal/eu_cookie_compliance": "^1.0.0-beta7",
        "drupal/menu_admin_per_menu": "^1.0.0-rc1",
        "drupal/google_analytics": "^2.1.0",
        "drupal/linkit": "^4.3.0",
        "drupal/slick_entityreference": "^1.0.0",
        "drupal/entity_embed": "^1.0.0-beta2",
        "drupal/panels": "^4.1.0",
        "drupal/menu_block": "^1.4.0",
        "drupal/rest_menu_items": "^1.3.0",
        "drupal/entity_reference_revisions": "^1.3.0",
        "drupal/samlauth": "^2.0.0-alpha1",
        "drupal/toolbar_menu": "^2.1.0",
        "drupal/node_clone": "1.x-dev",
        "drupal/csv_serialization": "^1.0.0",
        "drupal/embed": "^1.0.0",
        "drupal/externalauth": "^1.0.0",
        "drupal/sitemap": "^1.3.0",
        "drupal/rest_menu_tree": "^1.0.0",
        "drupal/diff": "^1.0.0-rc1",
        "drupal/ctools": "^3.0.0",
        "drupal/entity_pdf": "^1.0.0-alpha3",
        "drupal/migrate_plus": "^4.0.0-beta1",
        "drupal/menu_normalizer": "^1.0.0",
        "drupal/override_node_options": "^2.1.0",
        "drupal/smart_trim": "^1.0.0",
        "drupal/libraries": "3.x-dev",
        "drupal/redirect": "^1.0.0-beta1",
        "drupal/slick": "^1.0.0",
        "drupal/migrate_tools": "^4.0.0-beta1",
        "drupal/entity_browser": "^1.1.0",
        "drupal/better_exposed_filters": "^3.0.0-alpha3",
        "drupal/search_api": "^1.3.0",
        "drupal/core": "^8.3.7"
    },
    "require-dev": {},
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "optimize-autoloader": true
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": [],
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/custom/*/composer.json"
            ],
            "replace": false,
            "ignore-duplicates": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}